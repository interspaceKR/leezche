<div class="">
<a onclick="
  fetch('이벤트.html').then(function(response){
    response.text().then(function(text){
      document.querySelector('.interspace-main-grid-9').innerHTML = text;
      fetch('이벤트/이벤트메뉴').then(function(response){
        response.text().then(function(text){
          document.querySelector('.이벤트메뉴').innerHTML = text;
          fetch('이벤트/진행중인이벤트').then(function(response){
            response.text().then(function(text){
              document.querySelector('.인테리어내용').innerHTML = text;
                fetch('bottom-nav').then(function(response){
                  response.text().then(function(text){
                    document.querySelector('.bottom-nav').innerHTML = text;
    })})})})
  })})})});
  "><h4>진행중인이벤트</h4></a>
</div>
<div class="">
<a onclick="
  fetch('이벤트.html').then(function(response){
    response.text().then(function(text){
      document.querySelector('.interspace-main-grid-9').innerHTML = text;
      fetch('이벤트/이벤트메뉴').then(function(response){
        response.text().then(function(text){
          document.querySelector('.이벤트메뉴').innerHTML = text;
          fetch('이벤트/마감된이벤트').then(function(response){
            response.text().then(function(text){
              document.querySelector('.인테리어내용').innerHTML = text;
                fetch('bottom-nav').then(function(response){
                  response.text().then(function(text){
                    document.querySelector('.bottom-nav').innerHTML = text;
    })})})})
  })})})});
  "><h4>마감된이벤트</h4></a>
</div>
